{
    "name": "mlsql",
    "displayName": "mlsql",
    "description": "The Programming Language Designed For Big Data and AI",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/src/extension",
    "activationEvents": [
        "onLanguage:mlsql",
        "onCommand:mlsql.run"
    ],
    "publisher": "allwefantasy",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "tslint": "tslint -t verbose src/**/*.ts"
    },
    "contributes": {
        "commands": [
            {
                "command": "mlsql.run",
                "title": "Run MLSQL Code"
            }
        ],
        "languages": [
            {
                "id": "mlsql",
                "aliases": [
                    "MLSQL",
                    "mlsql"
                ],
                "extensions": [
                    "mlsql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mlsql",
                "scopeName": "main.mlsql",
                "path": "./syntaxes/mlsql.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "applicationinsights": "^0.19.0",
        "axios": "^0.21.1",
        "qs": "^6.10.1",
        "vscode-languageclient": "7.0.0"
    },
    "devDependencies": {
        "@types/node": "^16.7.1",
        "@types/qs": "^6.9.7",
        "@types/vscode": "^1.52.0",
        "@vscode/test-electron": "^1.6.1",
        "tslint": "^5.5.0",
        "typescript": "^3.8.3"
    }
}

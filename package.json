{
    "name": "mlsql",
    "displayName": "mlsql",
    "description": "The Programming Language Designed For Big Data and AI",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/src/extension",
    "activationEvents": [
        "onLanguage:mlsql",
        "onCommand:mlsql.run",
        "onNotebook:mlsql-notebook-provider"
    ],
    "publisher": "allwefantasy",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "tslint": "tslint -t verbose src/**/*.ts"
    },
    "contributes": {
        "commands": [
            {
                "command": "mlsql.run",
                "title": "MLSQL: Run"
            }
        ],
        "languages": [
            {
                "id": "mlsql",
                "aliases": [
                    "MLSQL",
                    "mlsql"
                ],
                "extensions": [
                    "mlsql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mlsql",
                "scopeName": "main.mlsql",
                "path": "./syntaxes/mlsql.tmLanguage.json"
            }
        ],
        "notebooks": [
            {
                "id":"mlsql-notebook",
                "type": "mlsql-notebook",
                "displayName": "MLSQL Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.mlsqlnb"
                    }
                ]
            }
        ]
    },
    "extensionDependencies": [
        "RandomFractalsInc.vscode-data-preview"
    ],
    "dependencies": {
        "@types/vscode-notebook-renderer": "^1.60.0",
        "applicationinsights": "^0.19.0",
        "axios": "^0.21.1",
        "json5-to-table": "^0.1.5",
        "qs": "^6.10.1",
        "uuid": "^8.3.2",
        "vscode-languageclient": "7.0.0"
    },
    "devDependencies": {
        "@types/node": "^16.7.1",
        "@types/qs": "^6.9.7",
        "@types/uuid": "^8.3.1",
        "@types/vscode": "^1.52.0",
        "@vscode/test-electron": "^1.6.1",
        "tslint": "^5.5.0",
        "typescript": "^3.8.3"
    }
}
